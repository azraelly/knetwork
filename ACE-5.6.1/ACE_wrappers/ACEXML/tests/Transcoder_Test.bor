# Makefile for building the Transcoder_Test exe with Borland C++ Make

NAME = Transcoder_Test

NO_FULL_PATH=1

OBJFILES = \
	$(OBJDIR)\Transcoder_Test.$(OBJ_EXT)

LFLAGS = \
        -L"." \
        -j"."  \
        -L"..\..\lib" \
        -j"..\..\lib" 

LIBFILES = \
	ACE$(LIB_DECORATOR).lib \
	ACEXML$(LIB_DECORATOR).lib \
	ACEXML_Parser$(LIB_DECORATOR).lib \
	$(DUMMY_VALUE_NOT_ENDING_IN_BACKSLASH)

!ifdef STATIC
LIB_FLAGS = \
        -DACE_AS_STATIC_LIBS
!else
!endif

CFLAGS = \
	-I"..\.." \
	-I"..\..\ACEXML\common" \
	$(LIB_FLAGS) \
	$(DLL_FLAGS)

CPPDIR = .
CDIR   = .

#
# Override defaults in outputdir.bor
#
INCLUDES_INSTALL=1

BASE_BINDIR = .

!include <$(ACE_ROOT)\include\makeinclude\build_exe.bor>

includes_install:
	@echo Nothing to install.

realclean:
        @-rem

