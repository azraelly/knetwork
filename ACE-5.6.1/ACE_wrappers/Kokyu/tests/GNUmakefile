$(REMAINING_TARGETS):
	@cd DSRT_MIF && $(MAKE) -f GNUmakefile.DSRT_MIF $(@)
	@cd EDF && $(MAKE) -f GNUmakefile.EDF $(@)
	@cd FIFO && $(MAKE) -f GNUmakefile.FIFO $(@)

.PHONY: DSRT_MIF-target
DSRT_MIF-target:
	@cd DSRT_MIF && $(MAKE) -f GNUmakefile.DSRT_MIF all

.PHONY: EDF-target
EDF-target:
	@cd EDF && $(MAKE) -f GNUmakefile.EDF all

.PHONY: FIFO-target
FIFO-target:
	@cd FIFO && $(MAKE) -f GNUmakefile.FIFO all

